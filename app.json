{
  "expo": {
    "name": "travel-guide",
    "slug": "travel-guide",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_API"
        }
      },
      "package": "com.krish2208.travelguide"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
